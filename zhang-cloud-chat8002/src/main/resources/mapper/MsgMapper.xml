<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.zhang.cloud.dao.MsgDAO">
    <insert id="insert" parameterType="com.zhang.cloud.entities.Message" useGeneratedKeys="true" keyProperty="id">
        insert into message(nickname,text,gmtCreated)
        values (#{nickname},#{text},now())
    </insert>
</mapper>